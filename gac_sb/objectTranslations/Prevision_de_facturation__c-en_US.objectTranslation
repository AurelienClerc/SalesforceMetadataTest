    <validationRules>
        <errorMessage><!-- Si un de ces trois champs est rempli, les autres doivent l&apos;être également : 
-  HGNF?
-  Montant HGNF
-  Date d&apos;HGNF --></errorMessage>
        <name>Canada_champs_HGNF_obligatoire</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Si les champs HGNF ? Date d&apos;HGNF ou montant HGNF sont remplis, le type de prévision ne doit pas être HGNF --></errorMessage>
        <name>Canada_check_type_HGNF</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Merci de remplir le champ % Prestation si le prestataire est renseigné --></errorMessage>
        <name>Champ_Percent_presta_obligatoire_si_pres</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Pour qu&apos;un dossier puisse être rejeté,
les champs ci-doivent être renseigner ainsi :
Type de prévision = &quot; Prévision de facturation &quot; et 
Montant prévu = &quot; 0 &quot; --></errorMessage>
        <name>Check_Dossier_rejete</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Le contrat associé à la planification doit être le même que celui associé à la mission --></errorMessage>
        <name>Check_contrat</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Merci de remplir une date de prévision d&apos;encaissement --></errorMessage>
        <name>Check_date_prevision_encaissement</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Si la prévision est facturée il doit y avoir une facture liée --></errorMessage>
        <name>Check_facture_associee_si_facturee</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- La date prévue ne peut être ultérieure à la date de prévision d&apos;encaissement.
La date de prévision d&apos;encaissement ne peut être antérieure à la date du jour. --></errorMessage>
        <name>Date_encaissement_superieure_date_prevue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- La saisie des champs suivants est obligatoire :
Type de facturation &amp; Motif attente facturation --></errorMessage>
        <name>Planif_TAT_Champs_Obligatoires</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Une planification de facturation ayant un statut &quot;prévue&quot; doit obligatoirement avoir une &quot;date prévue&quot; supérieure à la date de blocage mensuel de la facturation. --></errorMessage>
        <name>Planif_superieur_a_date_de_blocage</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Il est obligatoire de renseigner le champ &quot; Type d&apos;avoir &quot; sur les planif ayant une prévision HT inférieure à 0. --></errorMessage>
        <name>Type_davoir_obligatoire_planif_inf_a_0</name>
    </validationRules>
