            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AnneePassage</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>PremiereAnneeEconomie</elementReference>
                </rightValue>
            </conditions>
            <connector>
