        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MontantEconomieClient__c</fullName>
        <externalId>false</externalId>
        <formula>( TauxInitial__c -   TauxRectifie__c )* MasseSalarialeActuelle__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Montant de l&apos;économie Client</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>MontantEconomiePonderee__c</fullName>
        <externalId>false</externalId>
        <formula>MontantEconomieClient__c * Recours__r.Mission__r.Contrat__r.Taux_de_remuneration__c/100 * Recours__r.TauxSucces__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
