    </fields>
    <fields>
        <fullName>Mission__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK( &quot;/&quot;&amp;Recours__r.DossierSalarie__r.Mission__c , Recours__r.DossierSalarie__r.Mission__r.Name,&quot;_self&quot; )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
