    <fields>
        <fullName>Montant_RD_retenu_option1__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Depense_de_personnel__r.Actif__c = true, 
  
  IF(ISPICKVAL(Projet__r.Type__c, &apos;normalisation&apos;), 
    ((Depense_de_personnel__r.Salaire_charge_option1__c + Depense_de_personnel__r.Interessement_option1__c + Depense_de_personnel__r.Participation_option1__c) * Heures_RD_option1__c / Depense_de_personnel__r.Total_heures_travaillees_option1__c) * 0.5 * 1.3,
  
    CASE(Depense_de_personnel__r.RecordType.DeveloperName, 
      &apos;jeuneDocteur&apos;, 
      2 * (Depense_de_personnel__r.Salaire_charge_option1__c + Depense_de_personnel__r.Interessement_option1__c + Depense_de_personnel__r.Participation_option1__c) * Heures_RD_option1__c / Depense_de_personnel__r.Total_heures_travaillees_option1__c, 
      &apos;miseDisposition&apos;, 
      Depense_de_personnel__r.Salaire_charge_option1__c * Heures_RD_option1__c / Depense_de_personnel__r.Total_heures_travaillees_option1__c, 
      (Depense_de_personnel__r.Salaire_charge_option1__c + Depense_de_personnel__r.Interessement_option1__c + Depense_de_personnel__r.Participation_option1__c) * Heures_RD_option1__c / Depense_de_personnel__r.Total_heures_travaillees_option1__c
    )
  ) 
, 
0)</formula>
