    <validationRules>
        <fullName>INNO_Duree_entre_0_et_1</fullName>
        <active>true</active>
        <description>La durée de la TimeSheet doit être comprise entre 0 et 1</description>
        <errorConditionFormula>Duree__c &lt; 0 || Duree__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Duree__c</errorDisplayField>
        <errorMessage>La durée de la TimeSheet doit être comprise entre 0 et 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>INNO_Phase_et_Mission</fullName>
        <active>true</active>
        <description>Check si les timesheets ayant les phases Audit, Valorisation, Justification et Contrôle ont bien une mission.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Phase__c, &apos;Audit&apos;), ISPICKVAL(Phase__c, &apos;Valorisation&apos;), ISPICKVAL(Phase__c, &apos;Justification&apos;), ISPICKVAL(Phase__c, &apos;Contrôle&apos;)), ISBLANK(Mission_CIR__c))</errorConditionFormula>
        <errorMessage>Attention vous avez entré des TimeSheets avec une phase nécessitant une mission !</errorMessage>
    </validationRules>
