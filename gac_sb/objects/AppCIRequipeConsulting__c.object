        <externalId>false</externalId>
        <formula>IF( ISBLANK(Mission_CIR__r.Cout_standard_INNO_advance__c), 
   null, 
   CASE( TEXT(Role__c),
      &quot;Consultant scientifique&quot;, Mission_CIR__r.Cout_standard_INNO_advance__r.Cout_standard_Consultant__c,
      &quot;Consultant fiscaliste&quot;, Mission_CIR__r.Cout_standard_INNO_advance__r.Cout_standard_Consultant__c,
